{

    "base_url": "https://financialmodelingprep.com/api/v3",
    "base_url_v4": "https://financialmodelingprep.com/api/v4",
    "initial_filters": {
        "market_cap_min": 10000000000,
        "market_cap_max": 20000000000,
        "exclude_financial_sector": false,
        "roe": {
            "min_avg": 0.15,
            "min_each_year": 0.1,
            "years": 3
        }
    },
    "growth_quality": {
        "revenue_growth": {
            "min_cagr": 0.2,
            "magnitude_weight": 0.35,
            "consistency_weight": 0.35,
            "sustainability_weight": 0.3
        },
        "eps_growth": {
            "min_cagr": 0.15
        },
        "fcf_growth": {
            "min_cagr": 0.15
        }
    },
    "risk_quality": {
        "debt_to_equity_max": 2.0,
        "interest_coverage_min": 3.0,
        "weights": {
            "debt_metrics": 0.3,
            "working_capital": 0.25,
            "margin_stability": 0.25,
            "cash_flow_quality": 0.2
        }
    },
    "valuation": {
        "per_max": 30.0,
        "pbr_max": 5.0,
        "fcf_yield_min": 3e-14,
        "weights": {
            "per": 0.3,
            "pbr": 0.2,
            "fcf_yield": 0.3,
            "growth_adjusted": 0.2
        }
    },
    "sentiment": {
        "weights": {
            "insider_trading": 0.4,
            "earnings": 0.35,
            "social_sentiment": 0.25
        }
    },
    "target_rates": {
        "revenue": 0.2,
        "eps": 0.15,
        "fcf": 0.15
    },
    "scoring": {
        "normalization": {
            "winsorize_percentile": 0.05
        },
        "weights": {
            "growth_quality": 0.4,
            "risk_quality": 0.25,
            "valuation": 0.2,
            "sentiment": 0.15
        },
        "coherence_bonus": {
            "max_multiplier": 1.15,
            "min_metrics_required": 3
        }
    },
    "output": {
        "format": "excel",
        "filename_prefix": "enhanced_nasdaq_screening",
        "min_quality_score": 0.0,
        "max_stocks": 100
    },
    "concurrency": {
        "max_workers": 5,
        "request_delay": 0.06,
        "max_retries": 3
    },
    "logging": {
        "level": "INFO",
        "file": "enhanced_screener.log"
    },
